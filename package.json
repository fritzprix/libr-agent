    "@remote-dom/core": "^1.9.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-log": "~2.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.0.11",
    "fast-deep-equal": "^3.1.3",
    "groq-sdk": "^0.27.0",
    "immer": "^10.1.1",
