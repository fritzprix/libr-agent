{
  "name": "synaptic-flow",
  "private": false,
  "version": "0.1.0",
  "description": "Lightning Fast AI Companion - Tauri-based AI agent platform with MCP integration",
  "author": "fritzprix",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fritzprix/synaptic-flow.git"
  },
  "keywords": [
    "ai",
    "agent",
    "multi-agent",
    "orchestration",
    "mcp",
    "model-context-protocol",
    "react",
    "desktop",
    "shareable-config"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" --ignore-path .prettierignore",
    "tauri": "tauri",
    "diagnose": "echo 'System diagnostic...' && ldd --version && echo 'WebKit packages:' && dpkg -l | grep webkit",
    "dead-code": "unimported",
    "dead-code:init": "unimported --init",
    "refactor:validate": "pnpm lint && pnpm format && pnpm build && pnpm dead-code"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@cerebras/cerebras_cloud_sdk": "^1.35.0",
